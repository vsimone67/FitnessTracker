{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Udp",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://192.168.86.36:9400"
        }
      },
      {
        "Name": "Console"
      },
      {
        "Name": "Udp",
        "Args": {
          "remoteAddress": "192.168.86.232",
          "remotePort": 7071
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v2",
    "title": "Workout Micro Service",
    "version": "v2",
    "routePrefix": "docs",
    "includeSecurity": false
  },
  "ServiceBusSettings": {
    "ServerName": "192.168.86.37",
    "QueueName": "WorkoutQueue",
    "NumberOfRetries": "3",
    "RetryInterval": "5000"
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "Workout Service",
    "udpHost": "192.168.86.36",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "metrics": {
    "enabled": true,
    "influxdbUrl": "http://192.168.86.50:8086",
    "influxdbName": "fitnesstracker",
    "serverEnvironment": "k8",
    "applicationSuite": "fitnesstracker",
    "httpPolicyTimeout": 10,
    "flushInterval": 5,
    "reportInterval": 100
  }
}
